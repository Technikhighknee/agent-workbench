{
  "name": "@agent-workbench/types",
  "version": "0.1.0",
  "description": "MCP server for TypeScript language service integration. Get type errors, hover info, and go-to-definition.",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "types": "./dist/server.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "import": "./dist/server.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "start": "node dist/server.js",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "mcp",
    "ai-agents",
    "typescript",
    "language-service",
    "type-checking",
    "model-context-protocol"
  ],
  "author": "Jason Posch <Technikhighknee>",
  "license": "UNLICENSE",
  "engines": {
    "node": ">=22.0.0"
  },
  "devDependencies": {
    "@types/node": "^24.10.1"
  },
  "dependencies": {
    "@agent-workbench/core": "^0.1.0",
    "@modelcontextprotocol/sdk": "^1.23.0",
    "typescript": "^5.9.3",
    "zod": "^4.1.13"
  }
}
