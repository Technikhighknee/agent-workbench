{
  "name": "@agent-workbench/core",
  "version": "0.1.0",
  "description": "Shared utilities for agent-workbench packages",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "tsx": "^4.19.0",
    "typescript": "^5.7.2"
  }
}
