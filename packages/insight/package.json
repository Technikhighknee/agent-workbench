{
  "name": "@agent-workbench/insight",
  "version": "0.1.0",
  "description": "MCP server for comprehensive code understanding. One call to understand a file, module, or symbol.",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "insight": "./dist/server.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "import": "./dist/server.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "start": "node dist/server.js",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "mcp",
    "ai-agents",
    "insight",
    "code-understanding",
    "model-context-protocol"
  ],
  "author": "Jason Posch <Technikhighknee>",
  "license": "UNLICENSE",
  "engines": {
    "node": ">=22.0.0"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.23.0",
    "zod": "^4.1.13",
    "@agent-workbench/core": "^0.1.0",
    "@agent-workbench/syntax": "^0.3.0"
  }
}
