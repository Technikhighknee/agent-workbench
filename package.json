{
  "name": "agent-workbench",
  "version": "early-prototyping",
  "description": "Inspired by the concept of IDE â€“ translated into the world of AI-Agents.",
  "type": "module",
  "scripts": {
    "generate:docs": "npx tsx scripts/generate-docs.ts",
    "build": "npm run build --workspaces",
    "build:core": "npm run build -w @agent-workbench/core",
    "build:syntax": "npm run build -w @agent-workbench/syntax",
    "build:task-runner": "npm run build -w @agent-workbench/task-runner",
    "build:history": "npm run build -w @agent-workbench/history",
    "build:project": "npm run build -w @agent-workbench/project",
    "build:types": "npm run build -w @agent-workbench/types",
    "build:test-runner": "npm run build -w @agent-workbench/test-runner",
    "build:insight": "npm run build -w @agent-workbench/insight",
    "build:preview": "npm run build -w @agent-workbench/preview",
    "build:board": "npm run build -w @agent-workbench/board",
    "typecheck": "npm run typecheck --workspaces --if-present",
    "clean": "rm -rf packages/*/dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Technikhighknee/agent-workbench.git"
  },
  "workspaces": [
    "packages/core",
    "packages/syntax",
    "packages/history",
    "packages/project",
    "packages/types",
    "packages/test-runner",
    "packages/task-runner",
    "packages/insight",
    "packages/preview",
    "packages/board"
  ],
  "keywords": [
    "ai-agent",
    "mcp",
    "claude",
    "ide"
  ],
  "author": "Jason Posch <Technikhighknee>",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/Technikhighknee/agent-workbench/issues"
  },
  "homepage": "https://github.com/Technikhighknee/agent-workbench#readme",
  "devDependencies": {
    "@vitest/coverage-v8": "^4.0.14",
    "tsup": "^8.5.1",
    "vitest": "^4.0.14"
  }
}
