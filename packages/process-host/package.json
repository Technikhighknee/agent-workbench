{
  "name": "@agent-workbench/process-host",
  "version": "0.3.0",
  "description": "MCP server for AI agents to manage long-running processes. Never lose track of npm run dev again.",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "process-host": "./dist/server.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "import": "./dist/server.js"
    }
  },
  "files": [
    "dist",
    "schema"
  ],
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "tsc --watch",
    "test": "npx tsx test/smoke.ts",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "mcp",
    "ai-agents",
    "process-management",
    "model-context-protocol"
  ],
  "author": "Jason Posch <Technikhighknee>",
  "license": "UNLICENSE",
  "engines": {
    "node": ">=22.0.0"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.23.0",
    "zod": "^4.1.13"
  },
  "optionalDependencies": {
    "@cfworker/json-schema": "^4.1.1"
  }
}
